# Google Workspace OAuth Credentials - Business Account
# Get these from Google Cloud Console: https://console.cloud.google.com/
GOOGLE_OAUTH_CLIENT_ID_BUSINESS=your-business-client-id.apps.googleusercontent.com
GOOGLE_OAUTH_CLIENT_SECRET_BUSINESS=your-business-client-secret

# Google Workspace OAuth Credentials - Personal Account
# Get these from Google Cloud Console: https://console.cloud.google.com/
GOOGLE_OAUTH_CLIENT_ID_PERSONAL=your-personal-client-id.apps.googleusercontent.com
GOOGLE_OAUTH_CLIENT_SECRET_PERSONAL=your-personal-client-secret

# Google AI Studio API Key
# Get this from Google AI Studio: https://aistudio.google.com/app/apikey
GOOGLE_AI_API_KEY=your-google-ai-api-key-here

# Optional: Google Vertex AI (if using Vertex instead of AI Studio)
GOOGLE_VERTEX_PROJECT_ID=your-project-id
GOOGLE_VERTEX_LOCATION=us-central1
